import java.util.Scanner;

public class example {
	String readString() {
		StringBuffer sb = new StringBuffer();
		Scanner s = new Scanner(System.in);
		while (true) {
			String line = s.nextLine();
			if (line.length() == 1 && line.charAt(0) == ';')
				break;
		}
		return
	}
