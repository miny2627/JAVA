import java.util.Scanner;

public class example {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner s = new Scanner(System.in);
		String abc[] = {"가위", "바위", "보"};
		
		while(true) {
			System.out.print("가위(1), 바위(2), 보(3), 끝내기(4)>>");
			int n = s.nextInt();
			if (n == 4)
				break;
			String user = abc[n];
	}

}
